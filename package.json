  "dependencies": {
    "canvas": "1.6.3",
    "colors-promise": "1.0.1",
    "lagrange-promise": "1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.0",
